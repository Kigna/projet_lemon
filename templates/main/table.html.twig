<table>
    <thead>
        <tr>
            <th>Titre</th>
            <th>Date de début</th>
            <th>Date de fin</th>
            <th>Créateur</th>
        </tr>
    </thead>
    <tbody>
        {% for evenement in evenements %}
            <tr>
                <td>{{ evenement.titre }}</td>
                <td>{{ evenement.dateDebut|date('Y-m-d') }}</td>
                <td>{{ evenement.dateFin|date('Y-m-d') }}</td>
                <td>{{ evenement.createur.nom }}</td>
            </tr>
            {# // <script> console.log(evenements) console.log('salut')</script> #}
        {% endfor %}
        
    </tbody>
</table>